{
    "name": "your/project-name",
    "description": "Your project description.",
    "type": "project",
    "require": {
      "php": "^7.3 || ^8.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^9.6"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "config": {
      "optimize-autoloader": true
    }
  }
  